    <asp:ContentPlaceHolder ID="head" runat="server">
          <script type="text/javascript">
            function sweet(ti,te,ty) {
                 swal({
                     title: ti, 
                     text: te, 
                     type: ty,
                     showCancelButton: false
                 });
            }
            function sweetURL(ti, te, ty, url) {
                swal({
                    title: ti,
                    text: te,
                    type: ty,
                    showCancelButton: false
                }, function () {
                    if (url != "") {
                        window.location.href = url;
                    } else {
                        return false;
                    }

                });
              }

              var myWindow;
                function oWindow(url, w, h) {
                    var width = w;
                    var height = h;
                    var left = parseInt((screen.availWidth / 2) - (width / 2));
                    var top = parseInt((screen.availHeight / 2) - (height / 2));
                    var windowFeatures = "width=" + width + ",height=" + height + ",status,resizable=no,left=" + left + ",top=" + top + "screenX=" + left + ",screenY=" + top;
                    myWindow = window.open(url, "subWind", windowFeatures);
                    myWindow.focus();
                }

                function oWindowscroll(url, w, h) {
                    var width = w;
                    var height = h;
                    var left = parseInt((screen.availWidth / 2) - (width / 2));
                    var top = parseInt((screen.availHeight / 2) - (height / 2));
                    var windowFeatures = "width=" + width + ",height=" + height + ",status,scrollbars,resizable=no,left=" + left + ",top=" + top + "screenX=" + left + ",screenY=" + top;
                    myWindow = window.open(url, "subWind", windowFeatures);
                    myWindow.focus();
                }

                function oWindowScroll(url, w, h) {
                    var width = w;
                    var height = h;
                    var left = parseInt((screen.availWidth / 2) - (width / 2));
                    var top = parseInt((screen.availHeight / 2) - (height / 2));
                    var windowFeatures = "width=" + width + ",height=" + height + ",status,scrollbars,resizable=no,left=" + left + ",top=" + top + "screenX=" + left + ",screenY=" + top;
                    myWindow = window.open(url, "subWind", windowFeatures);
                    myWindow.focus();
                }
          </script>
    </asp:ContentPlaceHolder>
