            <!-- sidebar menu -->       
            <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                <%
                    Dim htm As String = ""
                    Dim url As String = VirtualPathUtility.ToAbsolute("~/")
                    Dim baseUrl As String = Request.Url.GetLeftPart(UriPartial.Authority) + Request.ApplicationPath + "/"
                    Dim arrow As String = ""
                    Dim iarrow As Integer
                    Dim arrow2 As String = ""
                    Dim iarrow2 As Integer
                    Dim linkp As String = ""
                    Dim linkps As String = ""

                    htm += "<div class='menu_section'>"
                    htm += "<h3>&nbsp;</h3>"
                    htm += "</div>"

                    Try
                        If dtParent.Rows.Count > 0 Then
                            htm += "<ul class='nav side-menu'>"

                            For i As Integer = 0 To dtParent.Rows.Count() - 1
                                Try
                                    getChild(dtParent.Rows(i).Item("id"))
                                    If dtChild.Rows.Count > 0 Then
                                        arrow = " <span class='fa fa-chevron-down'></span></a>"
                                        linkp = ""
                                        iarrow = 1
                                    Else
                                        arrow = "</a></li>"
                                        linkp = "href='" & baseUrl & dtParent.Rows(i).Item("mnu_link") & "'"
                                        iarrow = 0
                                    End If
                                Catch ex As Exception
                                    dtChild.Reset()
                                End Try

                                htm += "<li><a " & linkp & "><i class='" & dtParent.Rows(i).Item("mnu_icon") & "'></i> " & dtParent.Rows(i).Item("mnu_Name") & arrow


                                If dtChild.Rows.Count > 0 Then
                                    htm += "<ul class='nav child_menu'>"
                                    For z As Integer = 0 To dtChild.Rows.Count - 1
                                        Try
                                            getChildSub(dtChild.Rows(z).Item("id"))
                                            If dtChildSub.Rows.Count > 0 Then
                                                arrow2 = " <span class='fa fa-chevron-down'></span></a>"
                                                linkps = ""
                                                iarrow2 = 1
                                            Else
                                                arrow2 = "</a></li>"
                                                linkps = "href='" & baseUrl & dtChild.Rows(z).Item("mnu_link") & "'"
                                                iarrow2 = 0
                                            End If
                                        Catch ex As Exception
                                            dtChildSub.Reset()
                                        End Try

                                        htm += "<li class='treeview'><a " & linkps & ">" & dtChild.Rows(z).Item("mnu_name") & arrow2

                                        If dtChildSub.Rows.Count > 0 Then
                                            htm += "<ul class='nav child_menu'>"
                                            For u As Integer = 0 To dtChildSub.Rows.Count - 1
                                                htm += "<li><a href=" & baseUrl & dtChildSub.Rows(u).Item("mnu_link") & ">" & dtChildSub.Rows(u).Item("mnu_name") & "</a></li>"
                                            Next
                                            htm += "</ul>"
                                        End If

                                        If iarrow2 = 1 Then
                                            htm += "</li>"
                                        Else
                                            htm += ""
                                        End If

                                    Next

                                    htm += "</ul>"
                                End If

                                If iarrow = 1 Then
                                    htm += "</li>"
                                Else
                                    htm += ""
                                End If
                            Next
                            htm += "</ul>"
                        End If


                    Catch ex As Exception

                    End Try
                %>

                <%=htm%>    
            </div>
            <!-- /sidebar menu -->

